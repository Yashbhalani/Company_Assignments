
@model List<SourceControlAssignment1.Models.User>


@{
    ViewBag.Title = "GetAllUsers";
}

<h2>Users</h2>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">City</th>
            <th scope="col">Email id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {          
            <tr>
                <td>@user.UsrName</td>
                <td>@user.City</td>
                <td>@user.Email</td>
            </tr>

        }

    </tbody>
</table>
